<portlet-app version="1.0"
             xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">
    <portlet>
        <description xml:lang="EN">WSS List Portlet</description>
        <portlet-name>WSSListPortlet</portlet-name>
        <display-name xml:lang="EN">WSSListPortlet</display-name>
        <portlet-class>com.sun.portal.portlets.wss.list.WSSListPortlet</portlet-class>

        <expiration-cache>-1</expiration-cache>

        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
            <portlet-mode>edit</portlet-mode>
        </supports>

        <supported-locale>en</supported-locale>

        <portlet-info>
            <title>WSSListPortlet</title>
            <short-title>WSSListPortlet</short-title>
            <keywords>WSSListPortlet</keywords>
        </portlet-info>
    </portlet>

    <portlet>
        <portlet-name>WSSAlertPortlet</portlet-name>
        <portlet-class>com.sun.portal.portlets.wss.alert.WSSAlertPortlet</portlet-class>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
            <portlet-mode>edit</portlet-mode>
        </supports>
        <supported-locale>en</supported-locale>
        <portlet-info>
            <title>WSSAlertPortlet</title>
            <short-title>WSSAlertPortlet</short-title>
            <keywords>WSSAlertPortlet</keywords>
        </portlet-info>
    </portlet>


    <portlet>
        <portlet-name>WSSWebSitesPortlet</portlet-name>
        <portlet-class>com.sun.portal.portlets.wss.webs.WSSWebsPortlet</portlet-class>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
            <portlet-mode>edit</portlet-mode>
        </supports>
        <supported-locale>en</supported-locale>
        <portlet-info>
            <title>WSSWebSitesPortlet</title>
            <short-title>WSSWebSitesPortlet</short-title>
            <keywords>WSSWebSitesPortlet</keywords>
        </portlet-info>
    </portlet>


    <portlet>
        <portlet-name>WSSMeetingsPortlet</portlet-name>
        <portlet-class>com.sun.portal.portlets.wss.meetings.WSSMeetingsPortlet</portlet-class>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
            <portlet-mode>edit</portlet-mode>
        </supports>
        <supported-locale>en</supported-locale>
        <portlet-info>
            <title>WSSMeetingstPortlet</title>
            <short-title>WSSMeetingsPortlet</short-title>
            <keywords>WSSMeetingsPortlet</keywords>
        </portlet-info>
    </portlet>

    <portlet>
        <portlet-name>WSSDocumentsPortlet</portlet-name>
        <portlet-class>com.sun.portal.portlets.wss.documents.WSSDocumentsPortlet</portlet-class>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
            <portlet-mode>edit</portlet-mode>
        </supports>
        <supported-locale>en</supported-locale>
        <portlet-info>
            <title>WSSDocumentsPortlet</title>
            <short-title>WSSDocumentsPortlet</short-title>
            <keywords>WSSDocumentsPortlet</keywords>
        </portlet-info>
    </portlet>


</portlet-app>
